(function(){
	'use strict';
	angular
		.module('webraovatApp')
		.controller('UserController', UserController);
	UserController.$inject = ['$http'];
	function UserController($http){
		vm.users = [];
		$http({
			url : "UserController?action=getall",
			method: "POST",
		}).then(function(response) {
			vm.users = response;
			console.log(vm.users);
        }, function(response) {
            console.log(response);
        });
	}
	
})();