(function(){
	'use strict';
	angular
		.module('webraovatApp')
		.controller('UserController', UserController);
	UserController.$inject = ['$http'];
	function UserController($http){
		var vm = this;
		vm.users = [];
		vm.load = load;
		
		vm.load();
		
		function load() {
			$http({
				url : "UserController?action=getall",
				method: "POST",
			}).then(function(response) {
				vm.users = response.data;
	        }, function(response) {
	            console.log(response);
	        });
		}
	}
	
})();