package BO;

import java.sql.SQLException;
import java.util.List;

import Bean.User;
import DAO.UserDAO;

public class UserBO {
	public UserDAO userDAO;
	
	public int createUser(User user) {
		return userDAO.createUser(user);
	}
	
	public List<User> findAll() throws SQLException{
		return userDAO.findAll();
	}
}
